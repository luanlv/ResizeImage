@(assetCDN: String => String, bigPipe: BigPipe, collection1: Pagelet, collection2: Pagelet, collection3: Pagelet, aside: Pagelet)(implicit pjaxVersion: pjax.PjaxVersion)
<!DOCTYPE html>
<html class="no-touch">
	<head>
		@HtmlStream.fromHtml(views.html.partials.import_t(assetCDN))
		<meta charset="utf-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<meta http-equiv="x-pjax-version" content="@pjaxVersion.value">
	</head>
	<body>
	@bigPipe.render { pagelets =>
		<div id="main" class="m-scene">
		<div class="container" id="container">
			<div class="cartBar">
				<span class="cartMeta">
					<ul>
						<li class="first"><strong>Số lượng: </strong><span class="simpleCart_quantity" onchange='console.log("ok");'>0</span></li>
						<li><strong>Tổng: </strong><span class="simpleCart_total">0</span></li>
						<li><a href="javascript:;" id="view_cart">Chi tiết</a></li>
						<li><a href="javascript:;" class="simpleCart_empty">Xóa tất cả</a></li>
						<li class="last"><a href="javascript:;" class="simpleCart_checkout">Thanh toán</a></li>
					</ul>
				</span>

				<div id="cart" class="hide_cart">
					<div class="simpleCart_items">
						<div class="cartHeaders">
							<div class="itemName">Tên sản phẩm</div>
							<div class="itemPrice">Giá</div>
							<div class="itemQuantity">số lượng</div>
						</div>
					</div>
				</div>
			</div>

			@HtmlStream.fromHtml(views.html.partials_extra.header(assetCDN))

			<div class="main">
				@pagelets("aside")

			<div id="pjax-container" class="content">
				<div class="c-left">
					<div class="content-top">
						<div class="i-slide ra-5 sa">
							<img src="@assetCDN(routes.Assets.versioned("imageDemo/slide1.jpg").url)" alt="">

						</div>

						<div class="i-info">

						</div>
					</div>


					<div class="content-bottom">

						<div class="collection  ra-3 bo sa">

							<div class="dataCol">
								<div class="menuCol">
										&rarr; Phần cứng, Thiết bị.</div>

								@pagelets("collection1")
							</div>

							<div class="dataCol">
								<div class="menuCol"> &rarr; LK Bán dẫn & Cảm biến</div>
								@pagelets("collection2")
							</div>

							<div class="dataCol">
								<div class="menuCol"> &rarr; LK Khác và Phụ kiện</div>
								@pagelets("collection3")
							</div>

						</div>


					</div>
				</div>

				<div class="c-right">
					<div class="support  ra-5 bo sa">
						Nhân viên 1: 0199999999
							<br>
						Nhân viên 2: 0100000000
							<br>
						......
							<br>
						......
							<br>
						......
							<br>
						......
					</div>

					<div class="map ra-5 bo sa">
						MAP or Facebook
					</div>
					<div class="scroll scroll-right">
						<ul>
							<li><div class="extra-product">1</div></li>
							<li><div class="extra-product2">2</div></li>
							<li><div class="extra-product">3</div></li>
							<li><div class="extra-product2">4</div></li>
							<li><div class="extra-product">5</div></li>
							<li><div class="extra-product2">6</div></li>
							<li><div class="extra-product">7</div></li>
							<li><div class="extra-product2">8</div></li>
							<li><div class="extra-product">9</div></li>
							<li><div class="extra-product2">10</div></li>
							<li><div class="extra-product">11</div></li>
							<li><div class="extra-product2">12</div></li>
						</ul>
					</div>
				</div>

			</div>
			<div class="footer"></div>
		</div>
		</div>
	@HtmlStream.fromHtml(views.html.partials.import_b(assetCDN))
	}

	</body>
</html>
